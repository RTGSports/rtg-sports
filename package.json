{
  "name": "rtg-sports",
  "private": true,
  "workspaces": ["packages/*"],
  "scripts": {
    "build": "pnpm -C packages/web build && pnpm -C packages/worker build",
    "dev": "concurrently -k \"pnpm -C packages/web dev\" \"pnpm -C packages/worker dev\"",
    "lint": "pnpm -C packages/web lint",
    "typecheck": "pnpm -C packages/web typecheck"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  }
}